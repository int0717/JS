.slideRight-enter
  transform translateX(-100%)
  position absolute
  top 0
.slideRight-leave-to
  transform translateX(100%)
  position absolute
  top 0
.slideLeft-enter
  transform translateX(100%)
  position absolute
  top 0
.slideLeft-leave-to
  transform translateX(-100%)
  position absolute
  top 0
.slideRight-enter-active,
.slideRight-leave-active,
.slideLeft-enter-active,
.slideLeft-leave-active
  transition: transform 1s
  